namespace HelloNs = hw:"http://www.hello.org/HelloNs"
{
	struct Complex
	{
		double Re;
		double Im;
	}

	struct More
	{
		int[] Integers;
		Complex[] Complexes;
		string Name;
	}

	enum SeatStatus
	{
		Free,
		Locked,
		Reserved
	}

	interface IHello
	{
		string SayHello(string name);
	}

	binding HttpSoap11
	{
		transport HTTP;
		encoding SOAP;
	}

	endpoint HelloService : IHello
	{
		binding HttpSoap11;
		address "http://www.tempuri.org";
	}
}
